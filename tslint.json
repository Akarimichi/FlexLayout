{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rules": {
      "semicolon": [
        true,
        "always"
      ],
      "quotemark": [
        true,
        "double",
        "jsx-double"
      ],
      "member-access": [true, "no-public"],
      "object-literal-sort-keys": false
    }
  }